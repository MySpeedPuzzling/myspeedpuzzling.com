{
    "name": "myspeedpuzzling/myspeedpuzzling.com",
    "type": "project",
    "description": "",
    "authors": [
        {
            "name": "Jan Mikeš",
            "email": "j.mikes@me.com"
        },
        {
            "name": "Simona Mikešová",
            "email": "s.mikesova@icloud.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/janmikes/auth0-symfony"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-uuid": "*",
        "api-platform/core": "^4.2",
        "auth0/symfony": "dev-main",
        "bacon/bacon-qr-code": "^3.0",
        "doctrine/doctrine-bundle": "^3.1",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.0",
        "intervention/image-symfony": "^1.1",
        "jetbrains/phpstorm-attributes": "^1.0",
        "league/flysystem-async-aws-s3": "^3.0",
        "league/oauth2-server-bundle": "^1.1",
        "liip/imagine-bundle": "^2.12",
        "lustmored/flysystem-v2-simple-cache-adapter": "^0.3.0",
        "monolog/monolog": "^3.0",
        "nelmio/cors-bundle": "^2.4",
        "nette/utils": "^4.0",
        "nyholm/psr7": "^1.8",
        "oneup/flysystem-bundle": "^4.9",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpoffice/phpspreadsheet": "^5.0",
        "psr-discovery/cache-implementations": "^1.1",
        "psr-discovery/container-implementations": "^1.1",
        "psr-discovery/event-dispatcher-implementations": "^1.1",
        "psr-discovery/http-client-implementations": "^1.1",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^2.0",
        "sentry/sentry-symfony": "^5.0",
        "stripe/stripe-php": "^19.0.0",
        "symfony/asset": "^8.0",
        "symfony/config": "^8.0",
        "symfony/console": "^8.0",
        "symfony/dependency-injection": "^8.0",
        "symfony/doctrine-messenger": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/expression-language": "8.*",
        "symfony/flex": "^2.1",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/http-client-contracts": "^3.4",
        "symfony/http-kernel": "^8.0",
        "symfony/intl": "^8.0",
        "symfony/lock": "8.*",
        "symfony/mailer": "^8.0",
        "symfony/mercure-bundle": "^0.4.2",
        "symfony/messenger": "^8.0",
        "symfony/mime": "^8.0",
        "symfony/monolog-bundle": "^4.0",
        "symfony/process": "^8.0",
        "symfony/property-access": "^8.0",
        "symfony/property-info": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/serializer": "^8.0",
        "symfony/string": "^8.0",
        "symfony/translation": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/ux-autocomplete": "*",
        "symfony/ux-chartjs": "^2.22",
        "symfony/ux-icons": "^2.22",
        "symfony/ux-live-component": "^2.21",
        "symfony/ux-turbo": "^2.1",
        "symfony/ux-twig-component": "^2.21",
        "symfony/validator": "^8.0",
        "symfony/web-link": "^8.0",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "^8.0",
        "twig/cssinliner-extra": "^3.18",
        "twig/extra-bundle": "^3.18",
        "twig/inky-extra": "^3.18",
        "twig/intl-extra": "^3.8",
        "twig/string-extra": "^3.7",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "league/flysystem-memory": "^3.31",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^12.5.1",
        "roave/security-advisories": "dev-latest",
        "slevomat/coding-standard": "^8.25.1",
        "squizlabs/php_codesniffer": "^4.0.1",
        "symfony/browser-kit": "^8.0",
        "symfony/css-selector": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/web-profiler-bundle": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SpeedPuzzling\\Web\\": "src",
            "SpeedPuzzling\\Tools\\": "tools"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedPuzzling\\Web\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=2G analyse",
        "cs": "vendor/bin/phpcs",
        "cs-fix": "vendor/bin/phpcbf",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.*",
            "docker": false
        }
    },
    "replace": {
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-uuid": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
