                                <li class="widget-list-item ms-2 me-2 me-md-4 ms-md-0">
                                    <a rel="nofollow" href="{{ path('add_time') }}" class="widget-list-link">{{ 'menu.add_time'|trans }}</a>
                                </li>
                                <li class="widget-list-item ms-2 me-2 me-md-4 ms-md-0">
